<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/all_background"
    tools:context=".activity.VehiclesGuide">
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/transparent"
        android:layout_above="@+id/adMobView"
        app:layout_constraintTop_toTopOf="parent">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/transparent">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10dp"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/rl_main">
    <ImageView
        android:id="@+id/img_back"
        android:layout_width="@dimen/_40dp"
        android:layout_height="@dimen/_40dp"
        android:layout_marginLeft="@dimen/_20dp"
        android:gravity="center"
        android:scaleType="fitCenter"
        android:layout_centerVertical="true"
        android:src="@drawable/ic_back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/vehicle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:gravity="center_vertical"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/_10dp"
        android:fontFamily="@font/reem_kufi"
        app:layout_constraintTop_toTopOf="parent"
        android:text="@string/vehicle_guide"
        android:textColor="@color/white"
        android:textSize="23dp" />
    </RelativeLayout>
    <ImageView
        android:id="@+id/img_line"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_15dp"
        android:src="@drawable/line_design"
        android:layout_marginLeft="@dimen/_40dp"
        android:layout_marginRight="@dimen/_40dp"
        android:layout_below="@+id/rl_main"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/_25dp"
            android:layout_below="@+id/img_line"
            android:layout_marginTop="@dimen/_15dp"
            android:layout_marginRight="@dimen/_25dp"
            android:layout_gravity="center_vertical"
            android:padding="@dimen/_5dp">
            <androidx.cardview.widget.CardView
                android:id="@+id/cv_sportscar"
                android:layout_width="match_parent"
                android:layout_height="140dp"
                app:cardBackgroundColor="@color/black"
                app:cardCornerRadius="8dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/weapon_back_left">
                    <RelativeLayout
                        android:id="@+id/ll_sportscar"
                        android:layout_width="130dp"
                        android:layout_height="140dp"
                        android:layout_centerVertical="true"
                        android:orientation="vertical">
                        <ImageView
                            android:id="@+id/img_sportscar"
                            android:layout_width="@dimen/_150dp"
                            android:scaleType="fitCenter"
                            android:layout_marginLeft="@dimen/_5dp"
                            android:layout_height="match_parent"
                            android:layout_centerVertical="true"
                            android:src="@drawable/ic_sports_car" />
                    </RelativeLayout>
                    <LinearLayout
                        android:id="@+id/ll_sportscar1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginBottom="@dimen/_10dp"
                        android:layout_alignParentRight="true">
                        <Button
                            android:id="@+id/txt_sportscar"
                            android:layout_width="@dimen/_120dp"
                            android:layout_height="@dimen/_30dp"
                            android:fontFamily="@font/reem_kufi"
                            android:gravity="center"
                            android:background="@drawable/blude_button"
                            android:text="@string/sportscar"
                            android:layout_gravity="end"
                            android:layout_marginTop="@dimen/_10dp"
                            android:layout_marginRight="@dimen/_15dp"
                            android:textColor="@color/white"
                            android:textStyle="bold"
                            android:textSize="@dimen/_15dp" />
                        <TextView
                            android:id="@+id/txt_sportscartopic"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_15dp"
                            android:text="@string/sportscartopic"
                            android:layout_marginRight="@dimen/_20dp"
                            android:gravity="end"
                            android:textColor="@color/white"
                            android:fontFamily="@font/reem_kufi"
                            android:textSize="@dimen/_15dp"/>

                    </LinearLayout>
                </RelativeLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cv_monstercar"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_150dp"
                android:layout_below="@+id/cv_sportscar"
                android:layout_marginTop="@dimen/_20dp"
                app:cardBackgroundColor="@color/black"
                app:cardCornerRadius="8dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/weaopn_background">
                    <RelativeLayout
                        android:id="@+id/ll_monstercar"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:orientation="vertical">
                        <ImageView
                            android:id="@+id/img_monstercar"
                            android:layout_width="@dimen/_150dp"
                            android:layout_height="match_parent"
                            android:padding="@dimen/_10dp"
                            android:scaleType="fitCenter"
                            android:layout_centerVertical="true"
                            android:src="@drawable/ic_monster_car" />
                    </RelativeLayout>
                    <LinearLayout
                        android:id="@+id/ll_monstercar1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginBottom="@dimen/_10dp"
                        android:layout_alignParentLeft="true">
                        <Button
                            android:id="@+id/txt_monstercar"
                            android:layout_width="130dp"
                            android:layout_height="@dimen/_30dp"
                            android:fontFamily="@font/reem_kufi"
                            android:gravity="center"
                            android:background="@drawable/blude_button"
                            android:text="@string/monstercar"
                            android:layout_gravity="start"
                            android:layout_marginTop="@dimen/_10dp"
                            android:layout_marginLeft="@dimen/_15dp"
                            android:textColor="@color/white"
                            android:textStyle="bold"
                            android:textSize="@dimen/_15dp" />
                        <TextView
                            android:id="@+id/txt_monstercartopic"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10dp"
                            android:text="@string/monstercartopic"
                            android:layout_marginLeft="@dimen/_15dp"
                            android:gravity="start"
                            android:textColor="@color/white"
                            android:fontFamily="@font/reem_kufi"
                            android:textSize="@dimen/_15dp"/>

                    </LinearLayout>
                </RelativeLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cv_moto"
                android:layout_width="match_parent"
                android:layout_height="140dp"
                android:layout_below="@+id/cv_monstercar"
                android:layout_marginTop="@dimen/_20dp"
                app:cardBackgroundColor="@color/black"
                app:cardCornerRadius="8dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/weapon_back_left">
                    <RelativeLayout
                        android:id="@+id/ll_moto"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_centerVertical="true"
                        android:orientation="vertical">
                        <ImageView
                            android:id="@+id/img_moto"
                            android:layout_width="155dp"
                            android:layout_height="match_parent"
                            android:scaleType="fitCenter"
                            android:padding="@dimen/_10dp"
                            android:layout_centerVertical="true"
                            android:src="@drawable/ic_moto" />
                    </RelativeLayout>
                    <LinearLayout
                        android:id="@+id/ll_moto1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginBottom="@dimen/_10dp"
                        android:layout_alignParentRight="true">
                        <Button
                            android:id="@+id/txt_moto"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/_30dp"
                            android:fontFamily="@font/reem_kufi"
                            android:gravity="center"
                            android:background="@drawable/blude_button"
                            android:text="@string/moto"
                            android:layout_gravity="end"
                            android:layout_marginTop="@dimen/_10dp"
                            android:layout_marginRight="@dimen/_15dp"
                            android:textColor="@color/white"
                            android:textStyle="bold"
                            android:textSize="@dimen/_15dp" />
                        <TextView
                            android:id="@+id/txt_mototopic"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_15dp"
                            android:text="@string/mototopic"
                            android:layout_marginRight="@dimen/_20dp"
                            android:gravity="end"
                            android:textColor="@color/white"
                            android:fontFamily="@font/reem_kufi"
                            android:textSize="@dimen/_15dp"/>

                    </LinearLayout>
                </RelativeLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cv_amphibian"
                android:layout_width="match_parent"
                android:layout_height="140dp"
                android:layout_below="@+id/cv_moto"
                android:layout_marginTop="@dimen/_20dp"
                app:cardBackgroundColor="@color/black"
                app:cardCornerRadius="8dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/weaopn_background">
                    <RelativeLayout
                        android:id="@+id/ll_amphibian"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:orientation="vertical">
                        <ImageView
                            android:id="@+id/img_amphibian"
                            android:layout_width="155dp"
                            android:layout_height="match_parent"
                            android:layout_centerVertical="true"
                            android:scaleType="fitCenter"
                            android:padding="@dimen/_10dp"
                            android:paddingLeft="@dimen/_10dp"
                            android:src="@drawable/ic_amphibian" />
                    </RelativeLayout>
                    <LinearLayout
                        android:id="@+id/ll_amphibian1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginBottom="@dimen/_10dp"
                        android:layout_alignParentLeft="true">

                        <Button
                            android:id="@+id/txt_amphibian"
                            android:layout_width="@dimen/_110dp"
                            android:layout_height="@dimen/_30dp"
                            android:layout_gravity="start"
                            android:layout_marginLeft="@dimen/_15dp"
                            android:layout_marginTop="@dimen/_10dp"
                            android:background="@drawable/blude_button"
                            android:fontFamily="@font/reem_kufi"
                            android:gravity="center"
                            android:text="@string/amphibian"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_15dp"
                            android:textStyle="bold" />
                        <TextView
                            android:id="@+id/txt_amphibiantopic"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_15dp"
                            android:text="@string/amphibiantopic"
                            android:layout_marginLeft="@dimen/_15dp"
                            android:gravity="start"
                            android:textColor="@color/white"
                            android:fontFamily="@font/reem_kufi"
                            android:textSize="@dimen/_15dp"/>

                    </LinearLayout>
                </RelativeLayout>
            </androidx.cardview.widget.CardView>

            <RelativeLayout
                android:id="@+id/ads_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/cv_amphibian">
                <com.facebook.ads.NativeAdLayout
                    android:id="@+id/native_ad_container"
                    android:layout_width="match_parent"
                    android:layout_marginTop="@dimen/_20dp"
                    android:visibility="gone"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" />
                <com.google.android.ads.nativetemplates.TemplateView
                    android:visibility= "gone"
                    android:id= "@+id/nativeTemplateView"
                    app:gnt_template_type="@layout/gnt_small_template_view"
                    android:layout_width= "match_parent"
                    android:layout_height= "wrap_content" />
            </RelativeLayout>
            <androidx.cardview.widget.CardView
                android:id="@+id/cv_jeep"
                android:layout_width="match_parent"
                android:layout_height="140dp"
                android:layout_below="@+id/ads_layout"
                android:layout_marginTop="@dimen/_20dp"
                app:cardBackgroundColor="@color/black"
                app:cardCornerRadius="8dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/weapon_back_left">
                    <RelativeLayout
                        android:id="@+id/ll_jeep"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_centerVertical="true"
                        android:orientation="vertical">
                        <ImageView
                            android:id="@+id/img_jeep"
                            android:layout_width="155dp"
                            android:layout_height="match_parent"
                            android:layout_centerVertical="true"
                            android:scaleType="fitCenter"
                            android:padding="@dimen/_10dp"
                            android:src="@drawable/ic_military_jeep" />
                    </RelativeLayout>
                    <LinearLayout
                        android:id="@+id/ll_jeep1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginBottom="@dimen/_10dp"
                        android:layout_alignParentRight="true">
                        <Button
                            android:id="@+id/txt_jeep"
                            android:layout_width="130dp"
                            android:layout_height="@dimen/_30dp"
                            android:fontFamily="@font/reem_kufi"
                            android:gravity="center"
                            android:background="@drawable/blude_button"
                            android:text="@string/jeep"
                            android:layout_gravity="end"
                            android:layout_marginTop="@dimen/_10dp"
                            android:layout_marginRight="@dimen/_15dp"
                            android:textColor="@color/white"
                            android:textStyle="bold"
                            android:textSize="@dimen/_15dp" />
                        <TextView
                            android:id="@+id/txt_jeeptopic"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_10dp"
                            android:text="@string/jeeptopic"
                            android:layout_marginRight="@dimen/_15dp"
                            android:gravity="end"
                            android:textColor="@color/white"
                            android:fontFamily="@font/reem_kufi"
                            android:textSize="@dimen/_15dp"/>

                    </LinearLayout>
                </RelativeLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cv_tuktuk"
                android:layout_width="match_parent"
                android:layout_height="140dp"
                android:layout_below="@+id/cv_jeep"
                android:layout_marginTop="@dimen/_20dp"
                app:cardBackgroundColor="@color/black"
                app:cardCornerRadius="8dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/weaopn_background">
                    <RelativeLayout
                        android:id="@+id/ll_tuktuk"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:orientation="vertical">
                        <ImageView
                            android:id="@+id/img_tuktuk"
                            android:layout_width="155dp"
                            android:layout_height="match_parent"
                            android:layout_centerVertical="true"
                            android:scaleType="fitCenter"
                            android:src="@drawable/ic_tuktuk" />
                    </RelativeLayout>
                    <LinearLayout
                        android:id="@+id/ll_tuktuk1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginBottom="@dimen/_10dp"
                        android:layout_alignParentLeft="true">
                        <Button
                            android:id="@+id/txt_tuktuk"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/_30dp"
                            android:fontFamily="@font/reem_kufi"
                            android:gravity="center"
                            android:background="@drawable/blude_button"
                            android:text="@string/tuktuk"
                            android:layout_gravity="start"
                            android:layout_marginTop="@dimen/_10dp"
                            android:layout_marginLeft="@dimen/_15dp"
                            android:textColor="@color/white"
                            android:textStyle="bold"
                            android:textSize="@dimen/_15dp" />
                        <TextView
                            android:id="@+id/txt_tuktuktopic"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_15dp"
                            android:text="@string/tuktuktopic"
                            android:layout_marginLeft="@dimen/_15dp"
                            android:gravity="start"
                            android:textColor="@color/white"
                            android:fontFamily="@font/reem_kufi"
                            android:textSize="@dimen/_15dp"/>

                    </LinearLayout>
                </RelativeLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/cv_van"
                android:layout_width="match_parent"
                android:layout_height="140dp"
                android:layout_below="@+id/cv_tuktuk"
                android:layout_marginTop="@dimen/_20dp"
                app:cardBackgroundColor="@color/black"
                app:cardCornerRadius="8dp">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/weapon_back_left">
                    <RelativeLayout
                        android:id="@+id/ll_van"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_centerVertical="true"
                        android:orientation="vertical">
                        <ImageView
                            android:id="@+id/img_van"
                            android:layout_width="155dp"
                            android:layout_height="match_parent"
                            android:layout_centerVertical="true"
                            android:padding="@dimen/_10dp"
                            android:scaleType="fitCenter"
                            android:src="@drawable/ic_van" />
                    </RelativeLayout>
                    <LinearLayout
                        android:id="@+id/ll_van1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:layout_marginBottom="@dimen/_10dp"
                        android:layout_alignParentRight="true">

                        <Button
                            android:id="@+id/txt_van"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/_30dp"
                            android:layout_gravity="end"
                            android:layout_marginTop="@dimen/_10dp"
                            android:layout_marginRight="@dimen/_15dp"
                            android:background="@drawable/blude_button"
                            android:fontFamily="@font/reem_kufi"
                            android:gravity="center"
                            android:text="@string/van"
                            android:textColor="@color/white"
                            android:textSize="@dimen/_15dp"
                            android:textStyle="bold" />
                        <TextView
                            android:id="@+id/txt_vantopic"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_15dp"
                            android:text="@string/vantopic"
                            android:layout_marginRight="@dimen/_15dp"
                            android:gravity="end"
                            android:textColor="@color/white"
                            android:fontFamily="@font/reem_kufi"
                            android:textSize="@dimen/_15dp"/>

                    </LinearLayout>
                </RelativeLayout>
            </androidx.cardview.widget.CardView>
        </RelativeLayout>
    </RelativeLayout>
    </androidx.core.widget.NestedScrollView>
    <RelativeLayout
        android:id="@+id/adMobView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"/>
</RelativeLayout>